      .userfeed__newpost-body
        .userfeed__newpost-avatar
          img(src=require("../../assets/images/img/avatar.jpg"), alt="")
        // Tab panes
        .tab-content.userfeed__newpost-tabcontent
          #newPost.tab-pane.active(role='tabpanel' aria-labelledby='newPost-tab')
            .form-group
              label.sr-only(for='simplenewpost') Create Post
              textarea#simplenewpost.form-control.form__control-noborder.form__placeholder-dark(rows='3', placeholder='Create a new post or use @ to mention someone')
          #pollPost.tab-pane(role='tabpanel' aria-labelledby='pollPost-tab')
            .form-group
              label.sr-only(for='newpollpost') Create Poll
              input#newpollpost.form-control.form__control-noborder.form__placeholder-dark(type='text' placeholder='What would you like to ask?')
            .newpoll__options-wrap
              .newpoll__options
                .form-group
                  label.sr-only(for='newpollOption01') Option
                  input#newpollOption01.form-control(type='text' placeholder='Option 1')
              .newpoll__option-add
                button.btn.btn__add.btn__add-polloption
                  svg(xmlns='http://www.w3.org/2000/svg' viewbox='0 0 15 15')
                    rect.a(width='1.875' height='15' rx='0.938' transform='translate(6.563)')
                    rect.a(width='1.875' height='15' rx='0.938' transform='translate(15 6.563) rotate(90)')
            .newpost__options-tag
              .input-group
                .input-group-prepend
                  span#options__tag.input-group-text Tags
                input.form-control(type='text' placeholder='Create tags for your topic' aria-label='Tags' aria-describedby='options__tag')
          #photoPost.tab-pane(role='tabpanel' aria-labelledby='photoPost-tab')
            .form-group
              label.sr-only(for='newphotopost') Create Photo Post
              input#newphotopost.form-control.form__control-noborder.form__placeholder-dark(type='text' placeholder='Create a new post or use @ to mention someone')
            .newphotopost__group
              .newphotopost__container
                .newphotopost__item
                  img(src=require("../../assets/images/img/photo.jpg"), alt="")
                  button.btn.btn__remove-small &times;
              .form-group
                label.btn.btn__add.btn__add-big(for="photopost")
                  img(src=require("../../assets/images/svg/plus-gray.svg"), alt="")
                input#photopost(type="file", name="")
            .newpost__options-tag
              .input-group
                .input-group-prepend
                  span#options__tag.input-group-text Tags
                input.form-control(type='text' placeholder='Create tags for your topic' aria-label='Tags' aria-describedby='options__tag')
          #videoPost.tab-pane(role='tabpanel' aria-labelledby='videoPost-tab')
            .form-group
              label.sr-only(for='newvideopost') Create Video Post
              input#newvideopost.form-control.form__control-noborder.form__placeholder-dark(type='text' placeholder='Create a new post or use @ to mention someone')
            .newvideo__add-link
              button.btn.btn__add.btn__add-big.btn__add-video
                img(src=require("../../assets/images/svg/plus-gray.svg"), alt="")
              .form-group
                label.sr-only(for='newvideolink') Video link
                input#newvideolink.form-control(type='text' placeholder='Paste a link for a video')
            .newpost__options-tag
              .input-group
                .input-group-prepend
                  span#options__tag.input-group-text Tags
                input.form-control(type='text' placeholder='Create tags for your topic' aria-label='Tags' aria-describedby='options__tag')
          #articlePost.tab-pane(role='tabpanel' aria-labelledby='articlePost-tab')
            .form-group
              label.sr-only(for='articlenewpost') Create Article Post
              textarea#articlenewpost.form-control.form__control-noborder.form__placeholder-dark(rows='3', placeholder='Create a new post or use @ to mention someone')
      .userfeed__newpost-footer
        .userfeed__newpost-privacy
          | Privacy
          .custom__select-with-img
            select.custom-select(name='privacy')
              option(value='public' data-img="/assets/images/icon-globe.svg") Public & Community
              option(value='private' data-img="/assets/images/icon-public.svg") Public
              option(value='onlyme' data-img="/assets/images/icon-private.svg") Private
            .custom__select-selected
              img.custom__select-selected-img(src=require("../../assets/images/svg/icon-globe.svg"), alt="")
              span.custom__select-selected-text Public &amp; Community
            ul.custom__select-list
              li.custom__select-list-item(value='publiccommunity')
                img.custom__select-img(src=require("../../assets/images/svg/icon-globe.svg"), alt="")
                .custom__select-list-text
                  h3.custom__select-name Public &amp; Community
                  p.m-0.custom__select-text Will be shared with their connections, followers, and posted on the community wall
              li.custom__select-list-item(value='public')
                img.custom__select-img(src=require("../../assets/images/svg/icon-public.svg"), alt="")
                .custom__select-list-text
                  h3.custom__select-name Public
                  p.m-0.custom__select-text Shared with connections &amp; followers
              li.custom__select-list-item(value='private')
                img.custom__select-img(src=require("../../assets/images/svg/icon-private.svg"), alt="")
                .custom__select-list-text
                  h3.custom__select-name Private
                  p.m-0.custom__select-text Shared only with connections
        button.btn.btn__primary(type="submit") Post
